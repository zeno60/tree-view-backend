{
  "name": "tree-view-backend",
  "version": "1.0.0",
  "description": "backend services for tree view ui",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "build-ts": "./node_modules/typescript/bin/tsc",
    "watch-ts": "./node_modules/typescript/bin/tsc -w",
