    "lint": "./node_modules/tslint/bin/tslint -c tslint.json src/**/*.ts",
    "docker-build": "docker build -t gcr.io/passport-tree-app-268922/tree-view-backend:latest .",
    "docker-push": "docker push gcr.io/passport-tree-app-268922/tree-view-backend:latest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "convict": "^6.2.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "tsyringe": "^4.1.0",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@types/convict": "^4.2.1",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.45",
    "@types/node": "^13.7.2",
    "@types/socket.io": "^2.1.4",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  }
